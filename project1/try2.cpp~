#include <sys/types.h>
#include <unistd.h>
#include <stdio.h>
#include <cstdlib>
int main(int argc,char *argv[])
{
	pid_t pid;
	pid = fork();
	if(pid<0)
	{
		printf("Erorr.\n");
	}
	else if (pid == 0)
		execlp("/bin/ls","ls",NULL);
	else
	{	
		wait(NULL);
		printf("Child Complete.\n");
	}
	return 0;
}


